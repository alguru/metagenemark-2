echo '>anydef' > tmpseq.fna
grep -v '>' /storage3/w/gms2/data/Mycobacterium_tuberculosis_H37Rv_uid57777/sequence.fasta | tr '[:lower:]' '[:upper:]' >> tmpseq.fna
/home/karl/repos/repos-gms2/bin/gmhmmp2 -M /home/karl/repos/repos-gms2/bin/mgm_11.mod -s tmpseq.fna -o itr_0.lst --mgm_type auto 
echo '>anydef' > tmpseq.fna
grep -v '>' /storage3/w/gms2/data/Mycobacterium_tuberculosis_H37Rv_uid57777/sequence.fasta | tr '[:lower:]' '[:upper:]' >> tmpseq.fna
/home/karl/repos/repos-gms2/bin/gmhmmp2 -M /home/karl/repos/repos-gms2/bin/mgm_11.mod -s tmpseq.fna -o itr_0.lst 
/home/karl/repos/repos-gms2/bin/biogem gms2-training -s tmpseq.fna -l itr_0.lst -m itr_1.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 0 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --run-motif-search false --genome-group D
/home/karl/repos/repos-gms2/bin/gmhmmp2 -m itr_1.mod -M /home/karl/repos/repos-gms2/bin/mgm_11.mod -s tmpseq.fna -o itr_1.lst --format train
/home/karl/repos/repos-gms2/bin/compp -n -a itr_0.lst -b itr_1.lst -G
/home/karl/repos/repos-gms2/bin/biogem gms2-training -s tmpseq.fna -l itr_1.lst -m itr_2.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group C --gc-upstr-len-rbs 20 --align right --gc-width-rbs 6 --gc-upstr-len-prom 20 --gc-width-prom 6 --gc-extended-sd TAAGGAGGTGA
/home/karl/repos/repos-gms2/bin/gmhmmp2 -m itr_2.mod -M /home/karl/repos/repos-gms2/bin/mgm_11.mod -s tmpseq.fna -o itr_2.lst --format train
/home/karl/repos/repos-gms2/bin/compp -n -a itr_1.lst -b itr_2.lst -G
/home/karl/repos/repos-gms2/bin/biogem experiment promoter-is-valid-for-bacteria --fnmod itr_2.mod --dist-thresh 14 --score-thresh 0.25 --window-size 1 --min-leaderless-percent 11 --min-leaderless-count 100 --fnlabels itr_2.lst --fnseq tmpseq.fna
/home/karl/repos/repos-gms2/bin/biogem experiment promoter-and-rbs-match --fnmod itr_2.mod --match-thresh 3
/home/karl/repos/repos-gms2/bin/biogem gms2-training -s tmpseq.fna -l itr_2.lst -m itr_3.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group C --gc-upstr-len-rbs 20 --align right --gc-width-rbs 6 --gc-upstr-len-prom 20 --gc-width-prom 6 --gc-extended-sd TAAGGAGGTGA
/home/karl/repos/repos-gms2/bin/gmhmmp2 -m itr_3.mod -M /home/karl/repos/repos-gms2/bin/mgm_11.mod -s tmpseq.fna -o itr_3.lst --format train
/home/karl/repos/repos-gms2/bin/compp -n -a itr_2.lst -b itr_3.lst -G
/home/karl/repos/repos-gms2/bin/biogem gms2-training -s tmpseq.fna -l itr_3.lst -m itr_4.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group C --gc-upstr-len-rbs 20 --align right --gc-width-rbs 6 --gc-upstr-len-prom 20 --gc-width-prom 6 --gc-extended-sd TAAGGAGGTGA
