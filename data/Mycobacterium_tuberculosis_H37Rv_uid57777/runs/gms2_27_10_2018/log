/home/karl/repos/repos-gms2/bin/biogem gms2-training -s tmpseq.fna -l itr_6.lst -m itr_7.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group C --gc-upstr-len-rbs 20 --align right --gc-width-rbs 6 --gc-upstr-len-prom 20 --gc-width-prom 6 --gc-extended-sd TAAGGAGGTGA
/home/karl/repos/repos-gms2/bin/gmhmmp2 -m itr_7.mod -M /home/karl/repos/repos-gms2/bin/mgm_11.mod -s tmpseq.fna -o itr_7.lst --format train
/home/karl/repos/repos-gms2/bin/compp -n -a itr_6.lst -b itr_7.lst -G
cp itr_7.mod GMS2.mod
/home/karl/repos/repos-gms2/bin/gmhmmp2 -m GMS2.mod -M /home/karl/repos/repos-gms2/bin/mgm_11.mod -s /storage3/w/gms2/data/Mycobacterium_tuberculosis_H37Rv_uid57777/sequence.fasta -o gms2.gff --format gff 
rm -f 
