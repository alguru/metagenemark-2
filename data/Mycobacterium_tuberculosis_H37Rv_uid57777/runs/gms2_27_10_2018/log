/home/karl/repos/repos-gms2/bin/gmhmmp2 -m itr_5.mod -M /home/karl/repos/repos-gms2/bin/mgm_11.mod -s tmpseq.fna -o itr_5.lst --format train
/home/karl/repos/repos-gms2/bin/compp -n -a itr_4.lst -b itr_5.lst -G
